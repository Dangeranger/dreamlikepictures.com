<script>
var iframes = jQuery('.vimeovideo');
		
jQuery('.play').bind('click', function () {	
	var clicked_id = myclasses = jQuery(this).attr('class').split(' ')[3];
	//alert(clicked_id);
	iframes.each(function() {
		      
		var player=$f(this);
		myclasses = jQuery(this).attr('class').split(' ');
		loopid = jQuery(this).attr('class').split(' ');
		if(clicked_id == loopid[2]){//alert(loopid[2]);
			 //player.api("play"); 
			 return;
		}else{ 
			if(myclasses[3] == "playing"){
				  player.api("pause");
				  jQuery("#iframe_src_"+loopid[2]).toggleClass('playing paused');
				  document.getElementById("iframe_container_"+loopid[2]).style.display = "none";
			}
		}	  
	});
	return false;
});		
	
function on(id) {
	document.getElementById("iframe_container_"+id).style.display = "block";
}
function off(id) {
    document.getElementById("iframe_container_"+id).style.display = "none";
}
function off(id) {
	//off_all();
    document.getElementById("iframe_container_"+id).style.display = "none";
}
jQuery(".share").click(function(){
	var sh_id = jQuery(this).attr('id');
	var res = sh_id.replace("share", "");
	jQuery("#share_"+res).show();
});
jQuery(".share-panel-close").click(function(){
	var id = jQuery(this).attr('id');
	var res = id.replace("cross_share_", "");
	//alert("#share_"+res);
	jQuery("#share_"+res).hide();
});
</script>	

		
