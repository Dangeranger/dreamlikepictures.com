<script>
$(window).scroll(function() {
    if ($(this).scrollTop() > 50 ) {
        $('.scrolltop:hidden').stop(true, true).fadeIn();
    } else {
        $('.scrolltop').stop(true, true).fadeOut();
    }
});
$(function(){$(".scroll").click(function(){$("html,body").animate({scrollTop:0},"1000");return false})})
</script>


<script type="text/javascript">


 function copyToClipboard(id) {
	 
   $('.s_copy').remove();
   var copyText = document.getElementById('share_link'+id);
   var elem = $('#share_link'+id);
	 
   var is_safari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent);
	
   if (navigator.userAgent.match(/ipad|ipod|iphone/i) || is_safari) {
				  var el = elem.get(0);
				  var editable = el.contentEditable;
				  var readOnly = el.readOnly;
				  el.contentEditable = true;
				  el.readOnly = true;
				  var range = document.createRange();
				  range.selectNodeContents(el);
				  var sel = window.getSelection();
				  sel.removeAllRanges();
				  sel.addRange(range);
				  el.setSelectionRange(0, 999999);
				  el.contentEditable = editable;
				  el.readOnly = readOnly;
	   var sid = "#share_"+id+ " .share-panel-label:nth-of-type(2)";
	  $(sid).append('<span class="s_copy">Copied</span>');
	}	 
	 else
	 {
		 copyText.select();  
	 }
	  
	  document.execCommand("copy");
	
	 //$( "<p>Copied</p>" ).insertAfter(sid);
  //alert("Copied the text: " + copyText.value);
}

</script>


<script>
$(document).ready(function(){
    $('.menu-main-menu-container #mobmenuleft li a').click(function(){
		var link = $(this).attr('href');
		 var hash_links = link.substr(link.indexOf("#"));
		if(link == 'https://dreamlikepictures.com/work/')
		{
		  window.location="https://dreamlikepictures.com/work/";
		}
		else if(link == 'https://www.facebook.com/dreamlikepictures/?ref=bookmarks')
		{
		  window.location="https://www.facebook.com/dreamlikepictures/?ref=bookmarks";
		}	
		else if(link == 'https://dreamlikepictures.com/instagram/')
		{
		  window.location="https://dreamlikepictures.com/instagram/";
		}	
		if(hash_links == '#section1'){
			window.location="https://dreamlikepictures.com/#section1";
		}	
		else if(hash_links == '#section2'){
			window.location="https://dreamlikepictures.com/#section2";
		}	
		else if(hash_links == '#section3'){
			window.location="https://dreamlikepictures.com/#section3";
        }
		else if(hash_links == '#section4'){
		  window.location="https://dreamlikepictures.com/#section4";
		}
		else if(hash_links == '#section5'){
		  window.location="https://dreamlikepictures.com/#section5";
		}
		else if(hash_links == '#section6'){
		  window.location="https://dreamlikepictures.com/#section6";
		}
		else if(hash_links == '#section7'){
		  window.location="https://dreamlikepictures.com/#section7";
		}
		else if(hash_links == '#section8'){
		  window.location="https://dreamlikepictures.com/#section8";
		}
		else if(hash_links == '#'){
		  window.location="https://dreamlikepictures.com//?s=";
		}
		else if(link == 'https://dreamlikepictures.com/our-work/'){
		       window.location="https://dreamlikepictures.com/our-work/";
		    }
		
	    $('.mob-menu-left-panel .mobmenu-left-bt').click();
    });
    
});
    
</script>



